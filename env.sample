# Redis Stack Configuration
# Copy this file to .env and update the values as needed

# Redis Authentication
REDIS_USERNAME=default
REDIS_PASSWORD=your_secure_password_here

# Redis Database Configuration
REDIS_DB=0
REDIS_PORT=6379

# Redis Stack Additional Configuration
REDIS_HOST=localhost
REDIS_TIMEOUT=5000

# Optional: Redis Stack UI Configuration
REDIS_STACK_UI_PORT=8001
REDIS_STACK_UI_HOST=0.0.0.0

# Optional: Persistence Configuration
REDIS_RDB_FILENAME=dump.rdb
REDIS_AOF_FILENAME=appendonly.aof

# Optional: Memory Configuration
REDIS_MAXMEMORY=2gb
REDIS_MAXMEMORY_POLICY=allkeys-lru 